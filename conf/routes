GET	/webjars/*file				controllers.WebJarAssets.at(file)
GET	/assets/*file				controllers.Assets.at(path="/public", file)

GET	/					controllers.Application.index()
POST	/login					controllers.Application.login()
GET	/logout					controllers.Application.logout()
GET	/profile				controllers.Application.profile()

GET	/about/allgemeines			controllers.Application.aboutAllgemeines()
GET	/about/fachschaften			controllers.Application.aboutFachschaften()
GET	/about/geschichte			controllers.Application.aboutGeschichte()

GET	/stapf					controllers.Application.stapf()

GET	/beschluesse				controllers.Application.beschluesse()
